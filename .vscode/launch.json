{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "firefox",
      "request": "launch",
      "name": "Firefox Debug",
      "url": "http://localhost:5173",
      "webRoot": "${workspaceFolder}/ai_animation",
      "sourceMapPathOverrides": {
        "webpack:///./src/*": "${webRoot}/*"
      },
      "runtimeArgs": [
        "--remote-debugging-port=9222"
      ],
      "sourceMaps": true
    }
  ]
}
